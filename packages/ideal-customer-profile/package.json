{
  "name": "ideal-customer-profile",
  "version": "0.1.0",
  "description": "AI-powered Ideal Customer Profile (ICP) generator for targeted marketing",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts",
    "dev": "tsup src/index.ts --format esm --dts --watch",
    "test": "vitest",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "ai-functions": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "keywords": [
    "icp",
    "ideal-customer-profile",
    "buyer-persona",
    "marketing",
    "sales",
    "ai",
    "targeting"
  ],
  "author": "Startups.Studio",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/StartupsStudio/ideal-customer-profile.git"
  },
  "homepage": "https://icp.do",
  "bugs": {
    "url": "https://github.com/StartupsStudio/ideal-customer-profile/issues"
  }
}
