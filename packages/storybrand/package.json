{
  "name": "storybrand",
  "version": "0.1.0",
  "description": "AI-powered StoryBrand framework generator for compelling brand narratives",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts",
    "dev": "tsup src/index.ts --format esm --dts --watch",
    "test": "vitest",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "ai-functions": "workspace:*",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "keywords": [
    "storybrand",
    "branding",
    "marketing",
    "narrative",
    "ai",
    "framework",
    "copywriting"
  ],
  "author": "Startups.Studio",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/StartupsStudio/storybrand.git"
  },
  "homepage": "https://storybrand.do",
  "bugs": {
    "url": "https://github.com/StartupsStudio/storybrand/issues"
  }
}
