{
  "name": "experimentation-machine",
  "version": "0.1.0",
  "description": "AI-powered experimentation framework for hypothesis testing and A/B experiments",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts",
    "dev": "tsup src/index.ts --format esm --dts --watch",
    "test": "vitest",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "ai-functions": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "keywords": [
    "experimentation",
    "ab-testing",
    "hypothesis",
    "experiments",
    "ice-framework",
    "rice-framework",
    "sample-size",
    "statistical-significance",
    "startup",
    "ai",
    "framework"
  ],
  "author": "Startups.Studio",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/StartupsStudio/experimentation-machine.git"
  },
  "homepage": "https://experimentation-machine.do",
  "bugs": {
    "url": "https://github.com/StartupsStudio/experimentation-machine/issues"
  }
}
