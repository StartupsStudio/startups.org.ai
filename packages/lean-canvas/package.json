{
  "name": "lean-canvas",
  "version": "0.1.0",
  "description": "AI-powered Lean Canvas generator for startup business models",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./react": {
      "import": "./dist/LeanCanvas.js",
      "types": "./dist/LeanCanvas.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts --external ai-functions && tsup src/LeanCanvas.tsx --format esm --dts",
    "build:react": "tsup src/LeanCanvas.tsx --format esm --dts",
    "dev": "tsup src/index.ts src/LeanCanvas.tsx --format esm --dts --watch --external ai-functions",
    "test": "vitest",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "ai-functions": "workspace:*"
  },
  "peerDependencies": {
    "react": ">=18.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "keywords": [
    "lean-canvas",
    "business-model",
    "startup",
    "entrepreneurship",
    "ai",
    "framework",
    "lean-startup"
  ],
  "author": "Startups.Studio",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/StartupsStudio/lean-canvas.git"
  },
  "homepage": "https://lean-canvas.do",
  "bugs": {
    "url": "https://github.com/StartupsStudio/lean-canvas/issues"
  }
}
