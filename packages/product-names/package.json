{
  "name": "product-names",
  "version": "0.1.0",
  "description": "AI-enhanced product name generator for SaaS, apps, and features",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./patterns": {
      "import": "./dist/patterns.js",
      "types": "./dist/patterns.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/patterns.ts --format esm --dts",
    "dev": "tsup src/index.ts src/patterns.ts --format esm --dts --watch",
    "test": "vitest",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "ai-functions": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "keywords": [
    "product",
    "name-generator",
    "branding",
    "saas",
    "feature",
    "ai",
    "naming"
  ],
  "author": "Startups.Studio",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/StartupsStudio/product-names.git"
  },
  "homepage": "https://product-names.do",
  "bugs": {
    "url": "https://github.com/StartupsStudio/product-names/issues"
  }
}
