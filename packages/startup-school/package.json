{
  "name": "startup-school",
  "version": "0.1.0",
  "description": "AI-powered startup building framework based on Y Combinator Startup School curriculum",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./curriculum": {
      "import": "./dist/curriculum.js",
      "types": "./dist/curriculum.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/curriculum.ts --format esm --dts",
    "dev": "tsup src/index.ts src/curriculum.ts --format esm --dts --watch",
    "test": "vitest",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "ai-functions": "workspace:*",
    "storybrand": "workspace:*",
    "lean-canvas": "workspace:*",
    "ideal-customer-profile": "workspace:*",
    "jobs-to-be-done": "workspace:*",
    "startup-names": "workspace:*",
    "product-names": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "keywords": [
    "startup",
    "y-combinator",
    "yc",
    "startup-school",
    "entrepreneurship",
    "mvp",
    "product-market-fit",
    "fundraising",
    "pitch",
    "ai"
  ],
  "author": "Startups.Studio",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/StartupsStudio/startup-school.git"
  },
  "homepage": "https://startup-school.do",
  "bugs": {
    "url": "https://github.com/StartupsStudio/startup-school/issues"
  }
}
